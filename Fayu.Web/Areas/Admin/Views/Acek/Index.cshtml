@model List<Acek>
@{
    ViewData["Title"] = "Incoming Checks";
}
@section VendorStyles {

    <link rel="stylesheet" href="~/vendor/libs/flatpickr/flatpickr.css" />
    <link rel="stylesheet" href="~/vendor/libs/select2/select2.css" />
}
@section VendorScripts {
    <script src="~/vendor/libs/cleavejs/cleave.js"></script>
    <script src="~/vendor/libs/moment/moment.js"></script>
    <script src="~/vendor/libs/flatpickr/flatpickr.js"></script>
    <script src="~/vendor/libs/select2/select2.js"></script>
}
@section PageScripts {
    <script src="~/js/form-layouts.js"></script>
}
@* ************** Content ************** *@
<h4 class="py-3 mb-4">
    <a href="/Admin/Dashboard/Index" class="text-muted fw-light">Dashboard /</a> <a href="/Admin/Acek/Index">Incoming Checks</a>
</h4>

<!-- Ajax Sourced Server-side -->
<div class="card">
    <div class="card-header d-flex justify-content-between align-items-center">
        <h5>Incoming Checks</h5>
        <div class="ms-auto">
            <a href="/Admin/Acek/Upsert" type="button" class="btn btn-primary">Add New Checks</a>
        </div>
    </div>
    <div class="card-datatable text-nowrap">
        <table class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>Date</th>
                    <th>Customer</th>
                    <th>Check/Promissory Note</th>
                    <th>Amount</th>
                    <th>Owner/Serial Number</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var obj in Model)
                {
                    <tr>
                        <td>@obj.AcekTarih</td>
                        <td>@obj.AcekMusteri </td>
                        <td>@obj.AcekSenet </td>
                        <td>@obj.AcekTutar </td>
                        <td>@obj.AcekSeriNo </td>
                        <td>
                            <div class="w-75 btn-group" role="group">
                                <a href="/Admin/Acek/Upsert?id=@obj.Id" class="btn btn-primary mx-2">
                                    <span class="mdi mdi-square-edit-outline"></span>
                                </a>
                                <a onClick="Delete('/admin/Acek/delete/@obj.Id')" class="btn btn-danger mx-2"> <span style="color:white;" class="mdi mdi-delete-outline"></span></a>
                            </div>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

@section Scripts {
    <script src="~/upsert/supplier.js"></script>
}
